<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roomateshopping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomateShopping">
        <activity
            android:name=".SettleCostsActivity"
            android:exported="false"
            android:parentActivityName=".SettleCostsActivity"/>
        <activity
            android:name=".CompletedItemsActivity"
            android:exported="false"
            android:parentActivityName=".HomePageActivity"/>
        <activity
            android:name=".ViewShoppingListActivity"
            android:exported="false"
            android:parentActivityName=".HomePageActivity"/>
        <activity
            android:name=".AddItemActivity"
            android:exported="false"
            android:parentActivityName=".HomePageActivity"/>
        <activity
            android:name=".HomePageActivity"
            android:exported="false" />
        <activity
            android:name=".RegistrationActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>